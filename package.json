{
  "name": "@alesniak/graphql-codegen-faker",
  "version": "1.0.1",
  "description": "GraphQL Code Generator plugin for generating mocks based on faker package.",
  "repository": {
    "type": "git",
    "url": "https://github.com/adamlesniak/graphql-codegen-faker.git",
    "directory": "packages/plugins/typescript/typescript"
  },
  "license": "MIT",
  "scripts": {
    "build": "bob build",
    "lint": "eslint **/*.ts",
    "test": "jest --no-watchman",
    "prettier": "prettier -w **/*.ts",
    "release": "release-please --repo-url=https://github.com/adamlesniak/graphql-codegen-faker",
    "prepare": "husky"
  },
  "dependencies": {
    "@graphql-codegen/visitor-plugin-common": "5.8.0",
    "auto-bind": "~4.0.0",
    "tslib": "~2.8.1"
  },
  "peerDependencies": {
    "graphql": "^0.12.0 || ^0.13.0 || ^14.0.0 || ^15.0.0 || ^16.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "engines": {
    "node": ">=16"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@auto-it/conventional-commits": "^11.3.0",
    "@eslint/js": "^9.28.0",
    "@faker-js/faker": "^9.8.0",
    "@graphql-codegen/plugin-helpers": "^5.1.0",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-codegen/testing": "^3.0.4",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "bob-the-bundler": "^7.0.1",
    "eslint": "^9.28.0",
    "eslint-config-landr": "1.5.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.12.0",
    "eslint-plugin-prettier": "^5.4.1",
    "globals": "^16.2.0",
    "graphql": "^16.11.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-diff": "^29.7.0",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "prettier-config-landr": "^1.0.1",
    "prettier-eslint": "^16.4.2",
    "release-please": "^17.0.0",
    "ts-jest": "^29.3.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  }
}
